<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.mapper.ReplyMapper">
	<select id="getReplies" resultType="com.demo.domain.Reply">
		select * from tbl_reply where board_id = #{board_id}
	</select>
	<select id="getReply" resultType="com.demo.domain.Reply">
		select * from tbl_reply where reply_id = #{reply_id}
	</select>
	
</mapper>